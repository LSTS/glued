start()
{
    modprobe -l > /dev/null 2>&1
    if [ $? -eq 0 ]; then
        echo 'Modprobe file found'
    else
        echo 'No modprobe file found'
        sync
        mount -o remount rw / > /dev/null 2>&1
        depmod
        sync
        mount -o remount ro / > /dev/null 2>&1
        echo 'Finish generating modules.dep'
    fi
}

stop()
{
    echo ''
}
